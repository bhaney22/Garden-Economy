}

@article{zhao_forecasting_2015,
	title = {Forecasting carbon dioxide emissions},
	volume = {160},
	doi = {10.1016/j.jenvman.2015.06.002},
	abstract = {This study extends the literature on forecasting carbon dioxide (CO2) emissions by applying the reduced-form econometrics approach of Schmalensee et al. (1998) to a more recent sample period, the post-1997 period. Using the post-1997 period is motivated by the observation that the strengthening pace of global climate policy may have been accelerated since 1997. Based on our parameter estimates, we project 25\% reduction in CO2 emissions by 2050 according to an economic and population growth scenario that is more consistent with recent global trends. Our forecasts are conservative due to that we do not have sufficient data to fully take into account recent developments in the global economy. (C) 2015 Elsevier Ltd. All rights reserved.},
