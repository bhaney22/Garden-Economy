@article{fernandez_environmental_2012,
	title = {The environmental {Kuznets} curve and equilibrium indeterminacy},
	volume = {36},
	doi = {10.1016/j.jedc.2012.05.004},
	abstract = {We illustrate that the existence of the environmental Kuznets curve (EKC) could be the result of indeterminacy of equilibria. In a simple neoclassical growth model with a single environmental externality in households' utility function and with public abatement, we characterize the condition on the parameters in preferences and in the pollution technology leading to local indeterminacy of equilibrium. Estimates of the crucial parameters of the model using data from economies that display the EKC satisfy the theoretical indeterminacy condition. We find that the presence of abatement activities is not a necessary condition for indeterminacy equilibria and hence, an EKC to arise. On the other hand, non-separability of consumption and pollution in the utility function is crucial. (C) 2012 Elsevier B.V. All rights reserved.},
	number = {11},
	journal = {Journal of Economic Dynamics \& Control},
